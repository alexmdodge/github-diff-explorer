{
  "name": "github-diff-explorer",
  "version": "1.2.1",
  "description": "An extension which modifies the github diff viewer to instead use a file explorer similar to that of BitBucket and Stash.",
  "main": "index.js",
  "scripts": {
    "start": "npm run build:dev:watch",
    "build:dev": "webpack --config webpack.dev.js",
    "build:dev:watch": "webpack --config webpack.dev.js --watch",
    "build:prod": "webpack --config webpack.prod.js",
    "release": "npm run build:prod && ./pack-release.sh",
    "test": "jest --config ./jest.config.json",
    "version:patch": "./bump-version.sh PATCH",
    "version:minor": "./bump-version.sh MINOR",
    "version:major": "./bump-version.sh MAJOR"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/alexmdodge/github-diff-explorer.git"
  },
  "keywords": [
    "github",
    "bitbucket",
    "diff-viewer",
    "explorer",
    "plugin",
    "extension"
  ],
  "author": "Alex Dodge",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alexmdodge/github-diff-explorer/issues"
  },
  "homepage": "https://github.com/alexmdodge/github-diff-explorer#readme",
  "config": {
    "name": {
      "script": "gde_script",
      "style": "gde_styles"
    },
    "paths": {
      "entry": "./src/script.js",
      "vendor": "./src/vendor.js",
      "output": "./extension/lib"
    }
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "7.10.4",
    "@babel/preset-env": "7.10.4",
    "babel-jest": "26.1.0",
    "babel-loader": "8.1.0",
    "css-loader": "3.6.0",
    "jest": "26.1.0",
    "jest-environment-jsdom": "26.1.0",
    "jest-environment-jsdom-global": "2.0.4",
    "mini-css-extract-plugin": "0.9.0",
    "style-loader": "1.2.1",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.0",
    "webpack-merge": "4.2.2"
  }
}
