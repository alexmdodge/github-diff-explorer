{
  "name": "github-diff-explorer",
  "version": "1.0.3",
  "description": "An extension which modifies the github diff viewer to instead use a file explorer similar to that of BitBucket and Stash.",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "start": "cross-env NODE_ENV=development webpack --progress --colors --watch --mode development",
    "build:dev": "cross-env NODE_ENV=development webpack --progress --colors --mode development",
    "build:prod": "cross-env NODE_ENV=production webpack --progress --colors --mode production",
    "release": "npm run build:prod && ./pack-release.sh",
    "bump-version-patch": "./bump-version.sh PATCH",
    "bump-version-minor": "./bump-version.sh MINOR",
    "bump-version-major": "./bump-version.sh MAJOR"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/alexmdodge/github-diff-explorer.git"
  },
  "keywords": [
    "github",
    "bitbucket",
    "diff-viewer",
    "explorer",
    "plugin",
    "extension"
  ],
  "author": "Alex Dodge",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alexmdodge/github-diff-explorer/issues"
  },
  "homepage": "https://github.com/alexmdodge/github-diff-explorer#readme",
  "dependencies": {
    "autoprefixer": "7.2.3",
    "babel-core": "6.26.0",
    "babel-jest": "22.0.4",
    "babel-loader": "7.1.2",
    "babel-preset-env": "1.6.1",
    "concurrently": "3.5.1",
    "cross-env": "5.1.4",
    "css-loader": "0.28.7",
    "deep-extend": "0.5.0",
    "http-server": "0.10.0",
    "mini-css-extract-plugin": "0.4.0",
    "node-sass": "4.8.3",
    "optimize-css-assets-webpack-plugin": "4.0.0",
    "postcss-cssnext": "3.0.2",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-import": "11.0.0",
    "postcss-loader": "2.0.9",
    "postcss-url": "7.3.0",
    "sass-loader": "6.0.7",
    "style-loader": "0.19.1",
    "uglify-es": "3.3.9",
    "uglifyjs-webpack-plugin": "1.2.4",
    "webpack": "4.5.0",
    "webpack-cli": "2.0.14"
  }
}
